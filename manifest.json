{
  "manifest_version": 2,
  "name": "Chrome extenstion",
  "version": "0.1.0",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "exclude_globs": [
        "https://www.google.com/search*"
      ],
      "js": [
        "script.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup.html"
  }
}
